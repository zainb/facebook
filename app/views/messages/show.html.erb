<h2>Fill to send message</h2>> 
<%= form_for(@message) do |f| %>
  <div class="field">
    <%= f.label :send_to %><br />
    <%= f.text_area :send_to %>
  </div>
  <div class="field">
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>