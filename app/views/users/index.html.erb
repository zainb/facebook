<h2>Other Users</h2>
<% @users.each do |user|%>
  <table class="table table-striped">
    <div class="user">
      <p>
        <strong><%=h user.email if user != current_user%></strong>
          <% unless (current_user.friends.where(:id => user.id).any? || current_user == user) %> 
    		  <%= link_to "Add Friend", add_friend_user_path(user), :method => :post, :class => "btn btn-primary"%>
     	  <% end %>
      </p>
    </div>
  </table>
<% end %>